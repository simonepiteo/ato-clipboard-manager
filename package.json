{
  "name": "clipboardManager",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "macos": "npm run podInstall && xed -b macos && npm start",
    "podInstall": "cd macos && pod install && cd ../",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky",
    "clear": "rm -rf node_modules package-lock.json && npm i",
    "prerelease": "git checkout develop",
    "release": "commit-and-tag-version",
    "postrelease": "git fetch --atomic . develop:main && git push --atomic --follow-tags origin main develop"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "^2.11.1",
    "eventemitter3": "^5.0.1",
    "i18next": "^25.2.1",
    "react": "19.0.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.78.2",
    "react-native-macos": "^0.78.3",
    "react-native-svg": "^15.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "commit-and-tag-version": "^12.5.1",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
